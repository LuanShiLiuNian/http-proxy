<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <Appenders>
        <Console name="CONSOLE" target="SYSTEM_OUT">
            <PatternLayout charset="GB2312" pattern="%highlight{[%-5p]} %-d{yyyy-MM-dd HH:mm:ss} -> %style{%M()}{red}:%L => %m%n" />
        </Console>
    </Appenders>

    <Loggers>
        <root level="INFO">
            <AppenderRef ref="CONSOLE" />
        </root>
    </Loggers>
</configuration>
